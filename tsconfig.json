{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "baseUrl": "./",
        "paths": {
            "*": ["./src/*"]
        },
        "jsx": "react-jsx",
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src", "App.tsx", "AppLoading.tsx", "Loading.tsx"],
    "exclude": ["node_modules", "build"],
    "extends": "expo/tsconfig.base"
}
